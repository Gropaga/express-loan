{
  "connection": {
    "user": "guest",
    "pass": "guest",
    "server": [
      "127.0.0.1"
    ],
    "port": 5672,
    "vhost": "%2f",
    "timeout": 1000,
    "failAfter": 30,
    "retryLimit": 400,
    "clientProperties": {
      "lib": "rabbot",
      "process": "node.js (www-app)",
      "environment": "development"
    }

  },
  "exchanges": [
    {
      "name": "loan-x",
      "type": "topic",
      "persistent": true
    }
  ],
  "queues": [
    {
      "name": "loan-request-q",
      "autoDelete": true,
      "subscribe": true
    },
    {
      "name": "loan-response-q",
      "autoDelete": true,
      "subscribe": true
    }

  ],
  "bindings": [
    {
      "exchange": "loan-x",
      "target": "loan-request-q",
      "keys": [
        "loan.request.*"
      ]
    },
    {
      "exchange": "loan-x",
      "target": "loan-response-q",
      "keys": [
        "loan.response.*"
      ]
    }
  ]
}